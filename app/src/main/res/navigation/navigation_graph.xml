<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="idv.ron.logindemo_android.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_mainFragment_to_lalalalaFragment"
            app:destination="@id/lalalalaFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="idv.ron.logindemo_android.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" />
    <fragment
        android:id="@+id/lalalalaFragment"
        android:name="idv.ron.logindemo_android.LalalalaFragment"
        android:label="fragment_lalalala"
        tools:layout="@layout/fragment_lalalala" />
</navigation>